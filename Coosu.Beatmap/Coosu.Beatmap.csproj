<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net48;netcoreapp3.1;net5.0;net6.0;net7.0</TargetFrameworks>
    <Description>Coosu.Beatmap is library to read and write osu!beatmap data of .osu file.</Description>
    <PackageTags>osu, osu!, osugame</PackageTags>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsTrimmable>true</IsTrimmable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Coosu.Shared\Coosu.Shared.csproj" />
  </ItemGroup>

</Project>
