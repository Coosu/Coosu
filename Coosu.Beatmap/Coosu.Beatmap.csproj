<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45;net472;netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
    <Description>Coosu.Beatmap is library to read and write osu!beatmap data of .osu file.</Description>
    <PackageTags>osu, osu!</PackageTags>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <AssemblyName Condition="'$(CI)'!='true'">local.Coosu.Beatmap</AssemblyName>
  </PropertyGroup>

  <ItemGroup Condition="$(TargetFramework) == 'net45'">
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Coosu.Shared\Coosu.Shared.csproj" />
  </ItemGroup>

</Project>
